CREATE TABLE row(
   prk_row_id INT PRIMARY KEY
);

CREATE TABLE table(
   prk_table_id INT PRIMARY KEY,
   col_id INT
);

CREATE TABLE xml(
   prk_xml_id INT PRIMARY KEY,
   table_id INT,
   data_id INT
);

CREATE TABLE col(
   prk_col_id INT PRIMARY KEY
);

CREATE TABLE data(
   prk_data_id INT PRIMARY KEY,
   row_id INT
);

