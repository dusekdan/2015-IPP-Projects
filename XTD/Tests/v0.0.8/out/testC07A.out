CREATE TABLE a(
   prk_a_id INT PRIMARY KEY,
   b1_id INT,
   b2_id INT,
   b3_id INT,
   b4_id INT,
   c_id INT,
   attribute INT,
   nvarchar NVARCHAR
);

CREATE TABLE b(
   prk_b_id INT PRIMARY KEY,
   float FLOAT,
   integer INT,
   bool BIT
);

CREATE TABLE c(
   prk_c_id INT PRIMARY KEY,
   value FLOAT
);

